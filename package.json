    "dev": "next dev",
    "db:deploy": "prisma db push",
    "build": "npx prisma generate && next build",
    "build:static": "npx prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
